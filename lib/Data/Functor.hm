module Data.Functor (class Functor, map, (<$>)) where

class Functor f where
  map :: forall a b. (a -> b) -> f a -> f b

infixl 4 map as <$>

instance Functor List where
  map = mapListImpl

foreign import mapListImpl :: forall a b. (a -> b) -> List a -> List b
